{
  "name": "school_roster",
  "version": "0.1.2",
  "description": "School Roster made with SvelteKit and Tauri",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "pnpm run download-model && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "tauri": "tauri",
    "download-model": "node scripts/download-model.js"
  },
  "license": "MIT",
  "dependencies": {
    "@tauri-apps/api": "^1",
    "follow-redirects": "^1.15.9",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "tauri-plugin-store-api": "github:tauri-apps/tauri-plugin-store#v1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tauri-apps/cli": "^1",
    "sass": "^1.77.8",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "svelte-dnd-action": "^0.9.50",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3"
  }
